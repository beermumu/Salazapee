<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="800.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AdminAllAccountController">
   <children>
      <Label layoutX="253.0" layoutY="9.0" text="Account Management" textFill="#ff8605">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <TableView fx:id="accountsTableView" layoutX="13.0" layoutY="63.0" prefHeight="344.0" prefWidth="774.0">
         <columns>
            <TableColumn fx:id="id" prefWidth="35.000030517578125" text="ID" />
            <TableColumn fx:id="firstName" prefWidth="79.0" text="First Name" />
            <TableColumn fx:id="lastName" prefWidth="97.00001525878906" text="Last Name" />
            <TableColumn fx:id="username" prefWidth="83.0" text="Username" />
            <TableColumn fx:id="address" prefWidth="131.0" text="Address" />
            <TableColumn fx:id="email" prefWidth="108.0" text="E - Mail" />
            <TableColumn fx:id="tel" minWidth="0.0" prefWidth="98.0" text="Telephone" />
            <TableColumn fx:id="details" prefWidth="140.0" text="Details" />
         </columns>
      </TableView>
      <Button fx:id="addBtn" layoutX="132.0" layoutY="437.0" mnemonicParsing="false" onAction="#clickAddButton" prefHeight="25.0" prefWidth="50.0" text="Add" />
      <Button fx:id="deleteBtn" layoutX="359.0" layoutY="437.0" mnemonicParsing="false" onAction="#deleteAccount" prefHeight="25.0" prefWidth="65.0" text="Delete" textFill="RED" />
      <Button layoutX="602.0" layoutY="437.0" mnemonicParsing="false" onAction="#clickBackButton" prefHeight="25.0" prefWidth="44.0" text="Back" />
   </children>
</AnchorPane>
